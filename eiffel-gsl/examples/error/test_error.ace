system  "test_error"

root TEST_ERROR : make

default -- section of the system

	assertion (all);
	debug (yes);
	trace (no);
	collect (yes); -- garbage collector ?
	no_style_warning (no);
	no_warning (no);
	verbose (no);
	manifest_string_trace (no);
	high_memory_compiler (yes);	

cluster -- section
	gsl: "${eiffel_libraries}/eiffel-gsl/loadpath.se"
	example: "."
	standard: "${path_lib}/loadpath.se"

end

